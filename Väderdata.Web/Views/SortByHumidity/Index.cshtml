            </th>
            <th>
                @Html.DisplayNameFor(model => model.Plats)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.SelectDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AverageHumidity)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Plats)
